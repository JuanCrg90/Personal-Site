{
  "extends": "airbnb",
  "plugins": [
    "react",
    "jsx-a11y",
    "import"
    ],
    "rules": {
    "react/react-in-jsx-scope": "off",
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "react/jsx-one-expression-per-line": [enabled, { "allow": "single-child" }]
    }
}
